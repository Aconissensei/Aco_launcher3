{
  "name": "CobbleMyst",
  "productName": "CobbleMyst",
  "version": "1.2.3",
  "description": "Launcher Minecraft pour le serveur CobbleMyst",
  "main": "src/app.js",
  "author": {
    "name": "luuxis, Aconis",
    "email": "discord aco131220",
    "url": "https://github.com/Aconissensei/Aco_launcher3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aconissensei/Aco_launcher3.git"
  },
  "scripts": {
    "start": "cross-env-shell NODE_ENV=dev electron .",
    "dev": "cross-env-shell DEV_TOOL=open nodemon --exec npm start",
    "build": "node build.js --obf=true --build=platform",
    "icon": "node build.js --icon=https://i.imgur.com/9aHlKus.png",
    "release": "electron-builder -mwl --publish always"
  },
  "license": "CCANC",
  "nodemonConfig": {
    "ext": "js,html,css",
    "ignore": "**/test"
  },
  "dependencies": {
    "electron-updater": "^6.1.8",
    "minecraft-java-core": "^4.1.4",
    "node-bdd": "^2.0.4",
    "node-fetch": "^2.7.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^30.5.1",
    "electron-builder": "^26.0.12",
    "javascript-obfuscator": "^4.1.0",
    "jimp": "^0.22.12",
    "nodemon": "^3.1.2",
    "png2icons": "^2.0.1"
  },
  "build": {
    "appId": "com.cobblemyst.launcher",
    "productName": "CobbleMyst",
    "directories": { "output": "dist" },
    "files": [
      "src/**/*",
      "package.json"
    ],
    "mac": { "target": ["dmg","zip"], "category": "public.app-category.games" },
    "win": { "target": ["nsis"] },
    "linux": { "target": ["AppImage"], "category": "Game" },
    "publish": [
      { "provider": "github", "owner": "Aconissensei", "repo": "Aco_launcher3" }
    ]
  }
}
